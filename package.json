{
  "name": "keep-note",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build:api": "turbo build --filter=@seinfra/api",
    "build:web": "turbo build --filter=@seinfra/web",
    "dev:api": "turbo dev --filter=@seinfra/api",
    "dev:web": "turbo dev --filter=@seinfra/web",
    "start:api": "turbo start --filter=@seinfra/api",
    "start:web": "turbo start --filter=@seinfra/web",
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "prepare": "husky"
  },
  "devDependencies": {
    "prettier-plugin-tailwindcss": "^0.6.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "3.2.5",
    "turbo": "^2.3.3"
  },
  "pnpm": {
    "overrides": {
      "follow-redirects@<=1.15.5": ">=1.15.6",
      "next@>=13.4.0 <14.1.1": ">=14.1.1",
      "tar@<6.2.1": ">=6.2.1",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "braces@<3.0.3": ">=3.0.3",
      "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
      "micromatch@<4.0.8": ">=4.0.8",
      "next@>=14.0.0 <14.2.10": ">=14.2.10",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "cookie@<0.7.0": ">=0.7.0",
      "find-my-way@>=5.5.0 <8.2.2": ">=8.2.2",
      "next@>=10.0.0 <14.2.7": ">=14.2.7",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "nanoid@<3.3.8": ">=3.3.8",
      "next@>=9.5.5 <14.2.15": ">=14.2.15",
      "next@>=14.0.0 <14.2.21": ">=14.2.21",
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,d.ts}": "turbo lint --",
    "**/*.{json,css,config.*,md}": "prettier --write"
  }
}
